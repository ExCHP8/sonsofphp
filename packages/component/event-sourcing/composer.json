{
    "name": "sonsofphp/event-sourcing",
    "type": "library",
    "description": "Event Sourcing library",
    "keywords": ["event-sourcing"],
    "homepage": "https://github.com/SonsOfPHP/event-sourcing",
    "license": "MIT",
    "authors": [
        {
            "name": "Joshua Estes",
            "email": "joshua@sonsofphp.com"
        }
    ],
    "support": {
        "issues": "https://github.com/SonsOfPHP/sonsofphp/issues",
        "forum": "https://github.com/orgs/SonsOfPHP/discussions"
    },
    "autoload": {
        "psr-4": {
            "SonsOfPHP\\Component\\EventSourcing\\": ""
        },
        "exclude-from-classmap": [
            "/Tests/"
        ]
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": ">=7.4",
        "doctrine/dbal": "^3.3",
        "sonsofphp/event-dispatcher": "^0.1@dev"
    },
    "suggest": {
        "symfony/messenger": "Allows you to handle events asynchronously",
        "symfony/uid": "Helps generate UUIDs and ULIDs"
    },
    "extra": {
        "branch-alias": {
            "dev-main": "0.1.x-dev"
        }
    }
}
